<% if @flag%>
	$('.err_req').empty();
	$('#request-email').val('');
	$('#request-phone').val('');
	$().toastmessage('showSuccessToast', "Запрос оправлен");
<% else %>
    $('#req_mess').empty().append("<%= j(render :partial => 'lic_requests/form', :locals=>{:request=>LicRequest.new, user=>:user})%>");
    $('.err_req').empty().append("<%= j(render :partial=>'lic_requests/errors')%>");
<% end %>