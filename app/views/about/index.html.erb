<section class="section-content section-about" id="section-about">
    <div class="container">
      <%data=DataContact.first%>
      <h2 class="section-title">
        О компании
      </h2>
      <div class="row">
        <div class="span4">
          <%if data.photo == "/photos/original/missing.png"%>
              <img alt="" src="/assets/img/gallery/16rct-l.jpg">
          <%else%>
              <%=image_tag data.photo%>
          <%end%>
        </div>
        <div class="span8">
          <blockquote>
            <p class="lead">
             <%=data.preview%> 
          </blockquote>
          <div class="row">
            <div class='span8'>
            	<%= raw data.about%>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
<%content_for :footer do %>
<span></span>
<%end%>