<% if @flag%>
	$('.err_feed').empty();
	$("#contact-name-surname").val('');
	$('#contact-email').val('');
	$('#contact-message').val('');
	$().toastmessage('showSuccessToast', "Спасибо, Ваше сообщение отправлено!");
<% else %>
    $('.contact-feed').empty().append("<%= j(render :partial => 'feedbacks/form', :locals => {:feedback => @feedback})%>");
    $('.err_feed').empty().append("<%= j(render :partial=>'feedbacks/errors')%>");
<% end %>